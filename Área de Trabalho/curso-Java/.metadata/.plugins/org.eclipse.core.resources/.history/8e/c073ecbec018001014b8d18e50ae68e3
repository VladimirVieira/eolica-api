package colecoes;

import java.util.HashSet;
import java.util.Set;
public class ConjuntoComportado {
	public static void main(String[] args) {
		Set<String> lista = new HashSet<String>();//dessa forma sรณ aceita o tipo String
		lista.add("Ana");
		lista.add("Carlos");
		lista.add("Pedro");
		lista.add("Luca");
	}

}
